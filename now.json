{
	"version": 2,
	"alias": "devprtcl.com",
	"builds": [
		{ "src": "dist/**", "use": "@now/static" },
		{ "src": "public/.well-known/**", "use": "@now/static" }
	],
	"routes": [
		{
			"src": "/.well-known/(.*)",
			"dest": "/public/.well-known/$1"
		},
		{
			"src": "/(.*)",
			"headers": {
				"cache-control": "public, max-age=86400, s-maxage=31536000"
			},
			"dest": "/dist/$1"
		}
	]
}
